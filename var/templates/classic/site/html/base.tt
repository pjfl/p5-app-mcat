<!doctype html>
<html lang="en">
<head>
   <meta charset="utf-8">
   <title>[% nav.title_abbrev _ ' - ' _ nav.title_entry %]</title>
   <meta name="viewport" content="width=device-width,initial-scale=1.0">
   <meta name="apple-mobile-web-app-capable" content="yes">
   <meta name="description" content="[% config.name %]">
   <meta name="keywords" content="[% config.name %]">
   <meta name="generator" content="[% config.name %] v[% version %]">
   <link rel="shortcut icon" href="[% uri_for('img/favicon.ico') %]">
   <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto|Roboto+Slab">
[% skin = context.session.skin || config.skin -%]
[% css = uri_for('css/mcat-' _ skin _ '.css') -%]
   <link rel="stylesheet" href="[% css %]" type="text/css">
   <script src="[% uri_for('js/mcat.js') %]"></script>
   <script src="https://code.highcharts.com/highcharts.js"></script>
</head>
<body id="body">
[% content -%]
</body>
</html>
